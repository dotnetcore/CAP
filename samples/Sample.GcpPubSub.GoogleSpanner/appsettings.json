{
  "Pubsub": {
    "ProjectId": "<project-id>",
    "VerificationToken": "<gcp-token>",
    "TopicId": "<gcp-topic-id>",
    "SubscriptionId": "<gcp-subscription-id>"
  },
  "ConnectionStrings": {
    "SpannerDB": "Data Source=projects/{project}/instances/{instance}/databases/{database}"
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug"
    }
  }
}
